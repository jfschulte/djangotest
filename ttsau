[32m+[m	[32m$.slidebars = function ( options ) {[m
[32m+[m
[32m+[m		[32m// ----------------------[m
[32m+[m		[32m// 001 - Default Settings[m
[32m+[m
[32m+[m		[32mvar settings = $.extend( {[m
[32m+[m			[32msiteClose: true, // true or false - Enable closing of Slidebars by clicking on #sb-site.[m
[32m+[m			[32mscrollLock: false, // true or false - Prevent scrolling of site when a Slidebar is open.[m
[32m+[m			[32mdisableOver: false, // integer or false - Hide Slidebars over a specific width.[m
[32m+[m			[32mhideControlClasses: false // true or false - Hide controls at same width as disableOver.[m
[32m+[m		[32m}, options );[m
[32m+[m
[32m+[m		[32m// -----------------------[m
[32m+[m		[32m// 002 - Feature Detection[m
[32m+[m
[32m+[m		[32mvar test = document.createElement( 'div' ).style, // Create element to test on.[m
[32m+[m		[32msupportTransition = false, // Variable for testing transitions.[m
[32m+[m		[32msupportTransform = false; // variable for testing transforms.[m
[32m+[m
[32m+[m		[32m// Test for CSS Transitions[m
[32m+[m		[32mif ( test.MozTransition === '' || test.WebkitTransition === '' || test.OTransition === '' || test.transition === '' ) supportTransition = true;[m
[32m+[m
[32m+[m		[32m// Test for CSS Transforms[m
[32m+[m		[32mif ( test.MozTransform === '' || test.WebkitTransform === '' || test.OTransform === '' || test.transform === '' ) supportTransform = true;[m
[32m+[m
[32m+[m		[32m// -----------------[m
[32m+[m		[32m// 003 - User Agents[m
[32m+[m
[32m+[m		[32mvar ua = navigator.userAgent, // Get user agent string.[m
[32m+[m		[32mandroid = false, // Variable for storing android version.[m
[32m+[m		[32miOS = false; // Variable for storing iOS version.[m
[32m+[m[41m		[m
[32m+[m		[32mif ( /Android/.test( ua ) ) { // Detect Android in user agent string.[m
[32m+[m			[32mandroid = ua.substr( ua.indexOf( 'Android' )+8, 3 ); // Set version of Android.[m
[32m+[m		[32m} else if ( /(iPhone|iPod|iPad)/.test( ua ) ) { // Detect iOS in user agent string.[m
[32m+[m			[32miOS = ua.substr( ua.indexOf( 'OS ' )+3, 3 ).replace( '_', '.' ); // Set version of iOS.[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32mif ( android && android < 3 || iOS && iOS < 5 ) $( 'html' ).addClass( 'sb-static' ); // Add helper class for older versions of Android & iOS.[m
[32m+[m
[32m+[m		[32m// -----------[m
[32m+[m		[32m// 004 - Setup[m
[32m+[m
[32m+[m		[32m// Site container[m
[32m+[m		[32mvar $site = $( '#sb-site, .sb-site-container' ); // Cache the selector.[m
[32m+[m
[32m+[m		[32m// Left Slidebar[m[41m	[m
[32m+[m		[32mif ( $( '.sb-left' ).length ) { // Check if the left Slidebar exists.[m
[32m+[m			[32mvar $left = $( '.sb-left' ), // Cache the selector.[m
[32m+[m			[32mleftActive = false; // Used to check whether the left Slidebar is open or closed.[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Right Slidebar[m
[32m+[m		[32mif ( $( '.sb-right' ).length ) { // Check if the right Slidebar exists.[m
[32m+[m			[32mvar $right = $( '.sb-right' ), // Cache the selector.[m
[32m+[m			[32mrightActive = false; // Used to check whether the right Slidebar is open or closed.[m
[32m+[m		[32m}[m
[32m+[m[41m				[m
[32m+[m		[32mvar init = false, // Initialisation variable.[m
[32m+[m		[32mwindowWidth = $( window ).width(), // Get width of window.[m
[32m+[m		[32m$controls = $( '.sb-toggle-left, .sb-toggle-right, .sb-open-left, .sb-open-right, .sb-close' ), // Cache the control classes.[m
[32m+[m		[32m$slide = $( '.sb-slide' ); // Cache users elements to animate.[m
[32m+[m[41m		[m
[32m+[m		[32m// Initailise Slidebars[m
[32m+[m		[32mfunction initialise () {[m
[32m+[m			[32mif ( ! settings.disableOver || ( typeof settings.disableOver === 'number' && settings.disableOver >= windowWidth ) ) { // False or larger than window size.[m[41m [m
[32m+[m				[32minit = true; // true enabled Slidebars to open.[m
[32m+[m				[32m$( 'html' ).addClass( 'sb-init' ); // Add helper class.[m
[32m+[m				[32mif ( settings.hideControlClasses ) $controls.removeClass( 'sb-hide' ); // Remove class just incase Slidebars was originally disabled.[m
[32m+[m				[32mcss(); // Set required inline styles.[m
[32m+[m			[32m} else if ( typeof settings.disableOver === 'number' && settings.disableOver < windowWidth ) { // Less than window size.[m
[32m+[m				[32minit = false; // false stop Slidebars from opening.[m
[32m+[m				[32m$( 'html' ).removeClass( 'sb-init' ); // Remove helper class.[m
[32m+[m				[32mif ( settings.hideControlClasses ) $controls.addClass( 'sb-hide' ); // Hide controls[m
[32m+[m				[32m$site.css( 'minHeight', '' ); // Remove minimum height.[m
[32m+[m				[32mif ( leftActive || rightActive ) close(); // Close Slidebars if open.[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32minitialise();[m
[32m+[m[41m		[m
[32m+[m		[32m// Inline CSS[m
[32m+[m		[32mfunction css() {[m
[32m+[m			[32m// Site container height.[m
[32m+[m			[32m$site.css( 'minHeight', '' );[m
[32m+[m			[32mvar siteHeight = parseInt( $site.css( 'height' ), 10 ),[m
[32m+[m			[32mhtmlHeight = parseInt( $( 'html' ).css( 'height' ), 10 );[m
[32m+[m			[32mif ( siteHeight < htmlHeight ) $site.css( 'minHeight', $( 'html' ).css( 'height' ) ); // Test height for vh support..[m
[32m+[m[41m			[m
[32m+[m			[32m// Custom Slidebar widths.[m
[32m+[m			[32mif ( $left && $left.hasClass( 'sb-width-custom' ) ) $left.css( 'width', $left.attr( 'data-sb-width' ) ); // Set user custom width.[m
[32m+[m			[32mif ( $right && $right.hasClass( 'sb-width-custom' ) ) $right.css( 'width', $right.attr( 'data-sb-width' ) ); // Set user custom width.[m
[32m+[m[41m			[m
[32m+[m			[32m// Set off-canvas margins for Slidebars with push and overlay animations.[m
[32m+[m			[32mif ( $left && ( $left.hasClass( 'sb-style-push' ) || $left.hasClass( 'sb-style-overlay' ) ) ) $left.css( 'marginLeft', '-' + $left.css( 'width' ) );[m
[32m+[m			[32mif ( $right && ( $right.hasClass( 'sb-style-push' ) || $right.hasClass( 'sb-style-overlay' ) ) ) $right.css( 'marginRight', '-' + $right.css( 'width' ) );[m
[32m+[m[41m			[m
[32m+[m			[32m// Site scroll locking.[m
[32m+[m			[32mif ( settings.scrollLock ) $( 'html' ).addClass( 'sb-scroll-lock' );[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32m// Resize Functions[m
[32m+[m		[32m$( window ).resize( function () {[m
[32m+[m			[32mvar resizedWindowWidth = $( window ).width(); // Get resized window width.[m
[32m+[m			[32mif ( windowWidth !== resizedWindowWidth ) { // Slidebars is running and window was actually resized.[m
[32m+[m				[32mwindowWidth = resizedWindowWidth; // Set the new window width.[m
[32m+[m				[32minitialise(); // Call initalise to see if Slidebars should still be running.[m
[32m+[m				[32mif ( leftActive ) open( 'left' ); // If left Slidebar is open, calling open will ensure it is the correct size.[m
[32m+[m				[32mif ( rightActive ) open( 'right' ); // If right Slidebar is open, calling open will ensure it is the correct size.[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m		[32m// I may include a height check along side a width check here in future.[m
[32m+[m
[32m+[m		[32m// ---------------[m
[32m+[m		[32m// 005 - Animation[m
[32m+[m
[32m+[m		[32mvar animation; // Animation type.[m
[32m+[m
[32m+[m		[32m// Set animation type.[m
[32m+[m		[32mif ( supportTransition && supportTransform ) { // Browser supports css transitions and transforms.[m
[32m+[m			[32manimation = 'translate'; // Translate for browsers that support it.[m
[32m+[m			[32mif ( android && android < 4.4 ) animation = 'side'; // Android supports both, but can't translate any fixed positions, so use left instead.[m
[32m+[m		[32m} else {[m
[32m+[m			[32manimation = 'jQuery'; // Browsers that don't support css transitions and transitions.[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Animate mixin.[m
[32m+[m		[32mfunction animate( object, amount, side ) {[m
[32m+[m[41m			[m
[32m+[m			[32m// Choose selectors depending on animation style.[m
[32m+[m			[32mvar selector;[m
[32m+[m[41m			[m
[32m+[m			[32mif ( object.hasClass( 'sb-style-push' ) ) {[m
[32m+[m				[32mselector = $site.add( object ).add( $slide ); // Push - Animate site, Slidebar and user elements.[m
[32m+[m			[32m} else if ( object.hasClass( 'sb-style-overlay' ) ) {[m
[32m+[m				[32mselector = object; // Overlay - Animate Slidebar only.[m
[32m+[m			[32m} else {[m
[32m+[m				[32mselector = $site.add( $slide ); // Reveal - Animate site and user elements.[m
[32m+[m			[32m}[m
[32m+[m[41m			[m
[32m+[m			[32m// Apply animation[m
[32m+[m			[32mif ( animation === 'translate' ) {[m
[32m+[m				[32mif ( amount === '0px' ) {[m
[32m+[m					[32mremoveAnimation();[m
[32m+[m				[32m} else {[m
[32m+[m					[32mselector.css( 'transform', 'translate( ' + amount + ' )' ); // Apply the animation.[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} else if ( animation === 'side' ) {[m
[32m+[m				[32mif ( amount === '0px' ) {[m
[32m+[m					[32mremoveAnimation();[m
[32m+[m				[32m} else {[m
[32m+[m					[32mif ( amount[0] === '-' ) amount = amount.substr( 1 ); // Remove the '-' from the passed amount for side animations.[m
[32m+[m					[32mselector.css( side, '0px' ); // Add a 0 value so css transition works.[m
[32m+[m					[32msetTimeout( function () { // Set a timeout to allow the 0 value to be applied above.[m
[32m+[m						[32mselector.css( side, amount ); // Apply the animation.[m
[32m+[m					[32m}, 1 );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} else if ( animation === 'jQuery' ) {[m
[32m+[m				[32mif ( amount[0] === '-' ) amount = amount.substr( 1 ); // Remove the '-' from the passed amount for jQuery animations.[m
[32m+[m				[32mvar properties = {};[m
[32m+[m				[32mproperties[side] = amount;[m
[32m+[m				[32mselector.stop().animate( properties, 400 ); // Stop any current jQuery animation before starting another.[m
[32m+[m			[32m}[m
[32m+[m[41m			[m
[32m+[m			[32m// Remove animation[m
[32m+[m			[32mfunction removeAnimation () {[m
[32m+[m				[32mselector.removeAttr( 'style' );[m
[32m+[m				[32mcss();[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// ----------------[m
[32m+[m		[32m// 006 - Operations[m
[32m+[m
[32m+[m		[32m// Open a Slidebar[m
[32m+[m		[32mfunction open( side ) {[m
[32m+[m			[32m// Check to see if opposite Slidebar is open.[m
[32m+[m			[32mif ( side === 'left' && $left && rightActive || side === 'right' && $right && leftActive ) { // It's open, close it, then continue.[m
[32m+[m				[32mclose();[m
[32m+[m				[32msetTimeout( proceed, 400 );[m
[32m+[m			[32m} else { // Its not open, continue.[m
[32m+[m				[32mproceed();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Open[m
[32m+[m			[32mfunction proceed() {[m
[32m+[m				[32mif ( init && side === 'left' && $left ) { // Slidebars is initiated, left is in use and called to open.[m
[32m+[m					[32m$( 'html' ).addClass( 'sb-active sb-active-left' ); // Add active classes.[m
[32m+[m					[32m$left.addClass( 'sb-active' );[m
[32m+[m					[32manimate( $left, $left.css( 'width' ), 'left' ); // Animation[m
[32m+[m					[32msetTimeout( function () { leftActive = true; }, 400 ); // Set active variables.[m
[32m+[m				[32m} else if ( init && side === 'right' && $right ) { // Slidebars is initiated, right is in use and called to open.[m
[32m+[m					[32m$( 'html' ).addClass( 'sb-active sb-active-right' ); // Add active classes.[m
[32m+[m					[32m$right.addClass( 'sb-active' );[m
[32m+[m					[32manimate( $right, '-' + $right.css( 'width' ), 'right' ); // Animation[m
[32m+[m					[32msetTimeout( function () { rightActive = true; }, 400 ); // Set active variables.[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m[41m			[m
[32m+[m		[32m// Close either Slidebar[m
[32m+[m		[32mfunction close( url, target ) {[m
[32m+[m			[32mif ( leftActive || rightActive ) { // If a Slidebar is open.[m
[32m+[m				[32mif ( leftActive ) {[m
[32m+[m					[32manimate( $left, '0px', 'left' ); // Animation[m
[32m+[m					[32mleftActive = false;[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( rightActive ) {[m
[32m+[m					[32manimate( $right, '0px', 'right' ); // Animation[m
[32m+[m					[32mrightActive = false;[m
[32m+[m				[32m}[m
[32m+[m[41m			[m
[32m+[m				[32msetTimeout( function () { // Wait for closing animation to finish.[m
[32m+[m					[32m$( 'html' ).removeClass( 'sb-active sb-active-left sb-active-right' ); // Remove active classes.[m
[32m+[m					[32mif ( $left ) $left.removeClass( 'sb-active' );[m
[32m+[m					[32mif ( $right ) $right.removeClass( 'sb-active' );[m
[32m+[m					[32mif ( typeof url !== 'undefined' ) { // If a link has been passed to the function, go to it.[m
[32m+[m						[32mif ( typeof target === undefined ) target = '_self'; // Set to _self if undefined.[m
[32m+[m						[32mwindow.open( url, target ); // Open the url.[m
[32m+[m					[32m}[m
[32m+[m				[32m}, 400 );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32m// Toggle either Slidebar[m
[32m+[m		[32mfunction toggle( side ) {[m
[32m+[m			[32mif ( side === 'left' && $left ) { // If left Slidebar is called and in use.[m
[32m+[m				[32mif ( ! leftActive ) {[m
[32m+[m					[32mopen( 'left' ); // Slidebar is closed, open it.[m
[32m+[m				[32m} else {[m
[32m+[m					[32mclose(); // Slidebar is open, close it.[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mif ( side === 'right' && $right ) { // If right Slidebar is called and in use.[m
[32m+[m				[32mif ( ! rightActive ) {[m
[32m+[m					[32mopen( 'right' ); // Slidebar is closed, open it.[m
[32m+[m				[32m} else {[m
[32m+[m					[32mclose(); // Slidebar is open, close it.[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// ---------[m
[32m+[m		[32m// 007 - API[m
[32m+[m[41m		[m
[32m+[m		[32mthis.slidebars = {[m
[32m+[m			[32mopen: open, // Maps user variable name to the open method.[m
[32m+[m			[32mclose: close, // Maps user variable name to the close method.[m
[32m+[m			[32mtoggle: toggle, // Maps user variable name to the toggle method.[m
[32m+[m			[32minit: function () { // Returns true or false whether Slidebars are running or not.[m
[32m+[m				[32mreturn init; // Returns true or false whether Slidebars are running.[m
[32m+[m			[32m},[m
[32m+[m			[32mactive: function ( side ) { // Returns true or false whether Slidebar is open or closed.[m
[32m+[m				[32mif ( side === 'left' && $left ) return leftActive;[m
[32m+[m				[32mif ( side === 'right' && $right ) return rightActive;[m
[32m+[m			[32m},[m
[32m+[m			[32mdestroy: function ( side ) { // Removes the Slidebar from the DOM.[m
[32m+[m				[32mif ( side === 'left' && $left ) {[m
[32m+[m					[32mif ( leftActive ) close(); // Close if its open.[m
[32m+[m					[32msetTimeout( function () {[m
[32m+[m						[32m$left.remove(); // Remove it.[m
[32m+[m						[32m$left = false; // Set variable to false so it cannot be opened again.[m
[32m+[m					[32m}, 400 );[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( side === 'right' && $right) {[m
[32m+[m					[32mif ( rightActive ) close(); // Close if its open.[m
[32m+[m					[32msetTimeout( function () {[m
[32m+[m						[32m$right.remove(); // Remove it.[m
[32m+[m						[32m$right = false; // Set variable to false so it cannot be opened again.[m
[32m+[m					[32m}, 400 );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32m// ----------------[m
[32m+[m		[32m// 008 - User Input[m
[32m+[m[41m		[m
[32m+[m		[32mfunction eventHandler( event, selector ) {[m
[32m+[m			[32mevent.stopPropagation(); // Stop event bubbling.[m
[32m+[m			[32mevent.preventDefault(); // Prevent default behaviour.[m
[32m+[m			[32mif ( event.type === 'touchend' ) selector.off( 'click' ); // If event type was touch, turn off clicks to prevent phantom clicks.[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32m// Toggle left Slidebar[m
[32m+[m		[32m$( '.sb-toggle-left' ).on( 'touchend click', function ( event ) {[m
[32m+[m			[32meventHandler( event, $( this ) ); // Handle the event.[m
[32m+[m			[32mtoggle( 'left' ); // Toggle the left Slidbar.[m
[32m+[m		[32m} );[m
[32m+[m[41m		[m
[32m+[m		[32m// Toggle right Slidebar[m
[32m+[m		[32m$( '.sb-toggle-right' ).on( 'touchend click', function ( event ) {[m
[32m+[m			[32meventHandler( event, $( this ) ); // Handle the event.[m
[32m+[m			[32mtoggle( 'right' ); // Toggle the right Slidbar.[m
[32m+[m		[32m} );[m
[32m+[m[41m		[m
[32m+[m		[32m// Open left Slidebar[m
[32m+[m		[32m$( '.sb-open-left' ).on( 'touchend click', function ( event ) {[m
[32m+[m			[32meventHandler( event, $( this ) ); // Handle the event.[m
[32m+[m			[32mopen( 'left' ); // Open the left Slidebar.[m
[32m+[m		[32m} );[m
[32m+[m[41m		[m
[32m+[m		[32m// Open right Slidebar[m
[32m+[m		[32m$( '.sb-open-right' ).on( 'touchend click', function ( event ) {[m
[32m+[m			[32meventHandler( event, $( this ) ); // Handle the event.[m
[32m+[m			[32mopen( 'right' ); // Open the right Slidebar.[m
[32m+[m		[32m} );[m
[32m+[m[41m		[m
[32m+[m		[32m// Close Slidebar[m
[32m+[m		[32m$( '.sb-close' ).on( 'touchend click', function ( event ) {[m
[32m+[m			[32mif ( $( this ).is( 'a' ) || $( this ).children().is( 'a' ) ) { // Is a link or contains a link.[m
[32m+[m				[32mif ( event.type === 'click' ) { // Make sure the user wanted to follow the link.[m
[32m+[m					[32mevent.stopPropagation(); // Stop events propagating[m
[32m+[m					[32mevent.preventDefault(); // Stop default behaviour[m
[32m+[m[41m					[m
[32m+[m					[32mvar link = ( $( this ).is( 'a' ) ? $( this ) : $( this ).find( 'a' ) ), // Get the link selector.[m
[32m+[m					[32murl = link.attr( 'href' ), // Get the link url.[m
[32m+[m					[32mtarget = ( link.attr( 'target' ) ? link.attr( 'target' ) : '_self' ); // Set target, default to _self if not provided[m
[32m+[m[41m					[m
[32m+[m					[32mclose( url, target ); // Close Slidebar and pass link target.[m
[32m+[m				[32m}[m
[32m+[m			[32m} else { // Just a normal control class.[m
[32m+[m				[32meventHandler( event, $( this ) ); // Handle the event.[m
[32m+[m				[32mclose(); // Close Slidebar.[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m[41m		[m
[32m+[m		[32m// Close Slidebar via site[m
[32m+[m		[32m$site.on( 'touchend click', function ( event ) {[m
[32m+[m			[32mif ( settings.siteClose && ( leftActive || rightActive ) ) { // If settings permit closing by site and left or right Slidebar is open.[m
[32m+[m				[32meventHandler( event, $( this ) ); // Handle the event.[m
[32m+[m				[32mclose(); // Close it.[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m[41m		[m
[32m+[m	[32m}; // End Slidebars function.[m
[32m+[m
[32m+[m[32m} ) ( jQuery );[m
